    <h1>All Product</h1>
    
    <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-2">
        <%for(data of KidsData){%>
         
            <div class="listing-card" style="width: 16rem;">
                <img src="<%=data.image_url%>" class="card-img-top" alt="listing_img" style="height: 16rem;">
                <div class="card-body cards">
                    <p class="card-text text-primary fs-5 ">
                    <b><%= data.product_name%></b>
                </p>
                </div>
                <div class="parent">
                    
                    <div class="child"> <a href="/kids/<%= data._id%>"><button class="btn btn-danger ">Details</button></a></div>
                    <div class="child">
                        <form method="POST" action="/kids/<%=data._id%>/?_method=DELETE">
                            <button type="submit" class="btn btn-danger">Remove</button>
                        </form>
                    </div>
                </div>  
            </div>
     
    
            <% }%>
    
